{
  "name": "nestjs-hero-grpc-sample-with-health-check",
  "version": "1.1.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "k8s": "kubectl apply -k k8s"
  },
  "dependencies": {
    "@grpc/proto-loader": "0.5.3",
    "@nestjs/common": "7.4.4",
    "@nestjs/core": "7.4.4",
    "@nestjs/microservices": "7.4.4",
    "class-transformer": "0.3.1",
    "class-validator": "0.11.0",
    "grpc": "^1.24.11",
    "grpc-health": "1.6.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.0",
    "rxjs": "6.5.3"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.2",
    "@nestjs/schematics": "7.2.1",
    "@nestjs/testing": "7.4.4",
    "@types/express": "4.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "10.17.3",
    "@types/supertest": "2.0.8",
    "@types/ws": "6.0.4",
    "grpc-man": "^1.6.1",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "24.2.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.5.4",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "typescript": "3.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "deploy-node-app": {
    "prod": {
      "context": "kubesail-jeff-tian",
      "registry": "index.docker.io/v1/",
      "type": "server",
      "port": 3001,
      "protocol": "https",
      "entrypoint": "dist/main.js",
      "registryUsername": "jefftian",
      "isPublic": true
    }
  }
}
